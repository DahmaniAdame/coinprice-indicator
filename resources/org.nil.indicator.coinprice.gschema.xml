<schemalist>

  <schema id="org.nil.indicator.coinprice" path="/org/nil/coinprice/">
 
    <key type="i" name="refresh">
        <range min="3" max="60"/>
        <default>10</default>
        <summary>Refresh interval</summary>
    </key>
 
    <key type="s" name="exchange">
        <default>'kraken'</default>
        <summary>Kraken exchange</summary>
    </key>

    <key type="s" name="assetpair">
        <default>'XXBTZUSD'</default>
        <summary>Asset pairs</summary>
    </key>
 
  </schema>
</schemalist>
